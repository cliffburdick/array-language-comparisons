def scanl(f: (a, a) -> a, init: a, xs: n => a) -> n => a given (a|Data, n|Ix) =
   scan' init \i acc. f(acc, xs[i])

def mco(nums: n => Nat) -> Nat given (n|Ix) = 
  maximum $ scanl max 0 $ scanl (\a b. b * (a + b)) 0 nums

-- Tests
a = [1,1,0,1,1,1]
b = [1,0,1,1,0,1]

mco(a) -- 3
mco(b) -- 2
